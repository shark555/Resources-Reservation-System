<?xml version="1.0"?>
<glade-interface>
  <!-- interface-requires gtk+ 2.6 -->
  <!-- interface-naming-policy toplevel-contextual -->
  <widget class="GtkWindow" id="AboutWindow">
    <property name="visible">True</property>
    <property name="title" translatable="yes">About</property>
    <property name="window_position">center</property>
    <signal name="delete_event" handler="OnWindowDeleteEvent"/>
    <child>
      <widget class="GtkFixed" id="fixed1">
        <property name="visible">True</property>
        <child>
          <widget class="GtkImage" id="logo">
            <property name="width_request">150</property>
            <property name="height_request">108</property>
            <property name="visible">True</property>
            <property name="pixbuf">logo.png</property>
          </widget>
          <packing>
            <property name="x">41</property>
            <property name="y">18</property>
          </packing>
        </child>
        <child>
          <widget class="GtkLabel" id="InfoLabel">
            <property name="width_request">237</property>
            <property name="height_request">172</property>
            <property name="visible">True</property>
            <property name="label" translatable="yes">System Rezerwacji Zasob&#xF3;w

Autorzy:
Cezary B&#x142;ajszczak
Mateusz Grela
Dariusz P&#x142;awecki
Rafa&#x142; Wa&#x142;kowski
</property>
            <property name="justify">center</property>
          </widget>
          <packing>
            <property name="x">1</property>
            <property name="y">138</property>
          </packing>
        </child>
        <child>
          <widget class="GtkButton" id="OKButton">
            <property name="label" translatable="yes">OK</property>
            <property name="width_request">100</property>
            <property name="height_request">42</property>
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="receives_default">True</property>
          </widget>
          <packing>
            <property name="x">58</property>
            <property name="y">362</property>
          </packing>
        </child>
        <child>
          <widget class="GtkLabel" id="versionLabel">
            <property name="width_request">145</property>
            <property name="height_request">28</property>
            <property name="visible">True</property>
            <property name="label" translatable="yes">wersja</property>
          </widget>
          <packing>
            <property name="x">41</property>
            <property name="y">316</property>
          </packing>
        </child>
      </widget>
    </child>
  </widget>
</glade-interface>
